#: src/Admin/WCPP_Product_Fields.php:30
msgid "Promote this product"
msgstr "Promuj ten produkt"

#: src/Admin/WCPP_Product_Fields.php:31
msgid "Check this box to mark as promoted product."
msgstr "Oznacz jako promowany."

#: src/Admin/WCPP_Product_Fields.php:44
msgid "Custom Title"
msgstr "Własny tytuł"

#: src/Admin/WCPP_Product_Fields.php:45
msgid "Enter a custom title for this promoted product."
msgstr "Wprowadź własny tytuł promowanego produktu."

#: src/Admin/WCPP_Product_Fields.php:59
msgid "Set expiration date"
msgstr "Ustaw datę wygaśnięcia"

#: src/Admin/WCPP_Product_Fields.php:60
msgid "Check this box to enable setting an expiration date."
msgstr "Zaznacz aby aktywować datę wygaśnięcia."

#: src/Admin/WCPP_Admin_Notices.php:27
msgid "The promoted product has been updated."
msgstr "Promowany produkt został zaktualizowany."

#: src/Admin/WCPP_Settings.php:33
msgid "Promoted Product"
msgstr "Promowany produkt"

#: src/Admin/WCPP_Settings.php:43
msgid "Promoted Product Settings"
msgstr "Ustawienia promowanego produktu"

#: src/Admin/WCPP_Settings.php:48
msgid "Promoted Product Prefix"
msgstr "Prefiks Promowanego Produktu"

#: src/Admin/WCPP_Settings.php:50
msgid "Title for the promoted product, e.g., \"FLASH SALE:\""
msgstr "Tytuł dla promowanego produktu, np „FLASH SALE”"

#: src/Admin/WCPP_Settings.php:54
msgid "Background Color"
msgstr "Kolor tła"

#: src/Admin/WCPP_Settings.php:56
msgid "Choose a background color for the promoted product."
msgstr "Wybierz kolor tła dla promowanego produktu."

#: src/Admin/WCPP_Settings.php:60
msgid "Text Color"
msgstr "Kolor tekstu"

#: src/Admin/WCPP_Settings.php:62
msgid "Choose a text color for the promoted product."
msgstr "Wybierz kolor dla promowanego tekstu."

#: src/Admin/WCPP_Settings.php:93 src/Admin/WCPP_Settings.php:98
msgid "No active promoted product."
msgstr "Brak promowanego produktu."
